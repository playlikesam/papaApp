{
  "expo": {
    "name": "Mech Buddy",
    "slug": "mech-buddy",
    "version": "1.0.0",
    "entryPoint": "./node_modules/expo-router/entry",
    "orientation": "portrait",
    "icon": "./assets/icons/appLogo.png",
    "scheme": "mechbuddy",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2F80ED"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/appLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.maitrim.mechbuddy",
      "label": "Mech Buddy",
      "versionCode": 4
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/icons/appLogo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "router": {
        "origin": "https://mechbuddy.dev/"
      },
      "eas": {
        "projectId": "e9ae33ca-9aa9-4f98-8576-5d7274d200cd"
      }
    }
  }
}
